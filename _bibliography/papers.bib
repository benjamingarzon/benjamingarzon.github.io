	file = {Garzón et al. - 2011 - Multiparametric analysis of magnetic resonance ima.pdf:C\:\\Users\\Garzon\\Zotero\\storage\\FCS6JWUM\\Garzón et al. - 2011 - Multiparametric analysis of magnetic resonance ima.pdf:application/pdf},
