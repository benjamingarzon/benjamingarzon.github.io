@article{garzon_multiparametric_2011,
	title = {Multiparametric analysis of magnetic resonance images for glioma grading and patient survival time prediction},
	volume = {52},
	issn = {0284-1851, 1600-0455},
	url = {http://journals.sagepub.com/doi/10.1258/AR.2011.100510},
	doi = {10.1258/AR.2011.100510},
	abstract = {Background: A systematic comparison of magnetic resonance imaging (MRI) options for glioma diagnosis is lacking.
Purpose: To investigate multiple MR-derived image features with respect to diagnostic accuracy in tumor grading and survival prediction in glioma patients. Material and Methods: T1 pre- and post-contrast, T2 and dynamic susceptibility contrast scans of 74 glioma patients with histologically conﬁrmed grade were acquired. For each patient, a set of statistical features was obtained from the parametric maps derived from the original images, in a region-of-interest encompassing the tumor volume. A forward stepwise selection procedure was used to ﬁnd the best combinations of features for grade prediction with a cross-validated logistic model and survival time prediction with a cox proportional-hazards regression.
Results: Presence/absence of enhancement paired with kurtosis of the FM (ﬁrst moment of the ﬁrst-pass curve) was the feature combination that best predicted tumor grade (grade II vs. grade III–IV; median AUC ¼ 0.96), with the main contribution being due to the ﬁrst of the features. A lower predictive value (median AUC ¼ 0.82) was obtained when grade IV tumors were excluded. Presence/absence of enhancement alone was the best predictor for survival time, and the regression was signiﬁcant (P , 0.0001).
Conclusion: Presence/absence of enhancement, reﬂecting transendothelial leakage, was the feature with highest predictive value for grade and survival time in glioma patients.},
	language = {en},
	number = {9},
	urldate = {2024-04-25},
	journal = {Acta Radiologica},
	author = {Garzón, Benjamín and Emblem, Kyrre E and Mouridsen, Kim and Nedregaard, Baard and Due-Tønnessen, Paulina and Nome, Terje and Hald, John K and Bjørnerud, Atle and Håberg, Asta K and Kvinnsland, Yngve},
	month = nov,
