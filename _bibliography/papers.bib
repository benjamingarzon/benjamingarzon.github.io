	number = {5},
	urldate = {2024-04-25},
	journal = {Journal of Magnetic Resonance Imaging},
	author = {Jensen, Line R. and Garzon, Benjamin and Heldahl, Mariann G. and Bathen, Tone F. and Lundgren, Steinar and Gribbestad, Ingrid S.},
	month = nov,
